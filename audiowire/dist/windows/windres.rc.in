AudioWire ICON "AudioWire.ico"
1 VERSIONINFO
FILEVERSION     ${AUDIOWIRE_VERSION_MAJOR},${AUDIOWIRE_VERSION_MINOR},0,0
PRODUCTVERSION  ${AUDIOWIRE_VERSION_MAJOR},${AUDIOWIRE_VERSION_MINOR},0,0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "080904E4"
    BEGIN
      VALUE "CompanyName", "AudioWire"
      VALUE "FileDescription", "AudioWire music player"
      VALUE "FileVersion", "${AUDIOWIRE_VERSION_DISPLAY}"
      VALUE "InternalName", "AudioWire"
      VALUE "LegalCopyright", "David Sansome"
      VALUE "OriginalFilename", "AudioWire.exe"
      VALUE "ProductName", "AudioWire"
      VALUE "ProductVersion", "${AUDIOWIRE_VERSION_DISPLAY}"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x809, 1252
  END
END
