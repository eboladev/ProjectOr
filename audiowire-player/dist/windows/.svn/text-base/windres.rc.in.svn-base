      VALUE "OriginalFilename", "clementine.exe"
      VALUE "ProductName", "Clementine"
      VALUE "ProductVersion", "${CLEMENTINE_VERSION_DISPLAY}"
    END
  END

  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x809, 1252
  END
END
